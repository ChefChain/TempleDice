{
  "version": 1,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    },
    {
      "src": "api/*.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
